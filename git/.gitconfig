[user]
	name = Dylan Goings
	email = dylan@dgoings.com
[mergetool]
	keepBackup = true
[alias]
	st = status
	ci = commit -m
	co = checkout
	coc = !git checkout $1 && git clean -df ui/node_modules/
	coy = !git checkout $1 && cd ui/ && rm -rf node_modules/ && yarn && :
	br = branch
	lg = "!source ~/.githelpers && pretty_git_log"
	rb = rebase -i
	pu = push
	pl = pull
	irb = !git rebase -i origin/master
	mg = merge
	mgff = merge --ff-only
	fe = fetch
	up = !git fetch origin && git rebase origin/master
	cleanup = !git remote prune origin && git gc && git clean -dfx
	sub = submodule
	df = diff --color
[credential]
	helper = osxkeychain
[push]
	default = simple

